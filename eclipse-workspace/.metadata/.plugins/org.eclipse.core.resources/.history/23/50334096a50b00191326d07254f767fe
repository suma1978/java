package Newpackage;

import java.math.BigInteger;
import java.util.Scanner;

public class day1and2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
     int[] arr= {10,2,3,4,7};
     int[] arr1=new int[arr.length];
     arr1=intArray(arr);
   // System.out.print(verifyno(arr,15	));
    // prime();
  
	}
	
	public static int[] intArray(int[] arr)
	{
		int int1=65;
		System.out.println(Integer.toHexString(int1));
		System.out.println(Integer.valueOf("230",5));
		int product=1;
		
		for(int i=0;i<arr.length;i++)
		{
			product=product*arr[i];
		}
		for(int i=0;i<arr.length;i++)
		{
			arr[i]=product/arr[i];
			System.out.println(arr[i]);
			
		}
		System.out.println(product);
		return arr;
	}

	public static String verifyno(int[] arr,int n)
	{
		boolean flag=false;
		for(int i=0;i<arr.length-1;i++)
		{
			for(int j=i+1;j<arr.length;j++)
			{
			if(arr[i]+arr[j]==n)
			{
				flag=true;
				return "YES";
			}
			else
			{
				flag=false;
				continue;
			}
			}
		}
		if(flag==false)
		{
			return "NO";
		}
		else
		{
			return "YES";
		}
	}
	public static void prime()
	{
		Scanner in = new Scanner(System.in);
	      BigInteger n = in.nextBigInteger();
	      in.close();
	      System.out.println(n.isProbablePrime(100) ? "prime" :"not prime");
	}
	
}
